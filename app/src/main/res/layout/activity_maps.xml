<fragment xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/map"
    android:name="com.google.android.gms.maps.SupportMapFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.rgome.bikestripapp.AppContent.MapsActivity">

    <Button
        android:id="@+id/btnStartTack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/selectCity"
        android:layout_marginTop="16dp"
        android:text="@string/btn_start_track" />

    <Button
        android:id="@+id/btnFinishTack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/selectCity"
        android:layout_marginTop="16dp"
        android:text="@string/btn_finish_track" />


</fragment>

